{
    "name": "kraenzle-ritter/sipper",
    "description": "Deal with SIPs",
    "keywords": [
        "kraenzle-ritter",
        "sipper"
    ],
    "homepage": "https://github.com/kraenzle-ritter/sipper",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Andreas Kraenzle",
            "email": "kraenzle@k-r.ch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "KraenzleRitter\\Sipper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KraenzleRitter\\Sipper\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "KraenzleRitter\\Sipper\\SipperServiceProvider"
            ],
            "aliases": {
                "Sipper": "KraenzleRitter\\Sipper\\SipperFacade"
            }
        }
    }
}
